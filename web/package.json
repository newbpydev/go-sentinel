  "type": "commonjs",
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@tailwindcss/cli": "^4.1.6",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "dotenv": "^16.5.0",
    "live-server": "^1.2.2",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.6"
  },
  "dependencies": {
    "express": "^5.1.0",
    "express-handlebars": "^8.0.1",
    "ws": "^8.18.2"
  }
}
