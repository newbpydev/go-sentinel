{
  "name": "web",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build:css": "npx @tailwindcss/cli -i ./static/css/tailwind.css -o ./static/css/tailwind.build.css --minify",
    "watch:css": "npx @tailwindcss/cli -i ./static/css/tailwind.css -o ./static/css/tailwind.build.css --watch",
    "test": "jest",
    "test:e2e": "playwright test",
    "dev": "node dev.server.js",
    "dev:watch": "concurrently \"npx tailwindcss -i static/css/tailwind.css -o static/css/tailwind.build.css --watch\" \"node dev.server.js\"",
    "test:watch": "npx playwright test --ui",
    "dev:fullwatch": "concurrently \"npx tailwindcss -i static/css/tailwind.css -o static/css/tailwind.build.css --watch\" \"node dev.server.js\" \"npx playwright test --ui\""
  },
  "keywords": [],
  "author": "",
